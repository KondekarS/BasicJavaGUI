import java.awt.Frame;
import java.awt.Label;
import java.awt.FlowLayout;

class Main extends Frame{
  public Main()
  {
    Label lbl = new Label("Hello World");
    add(lbl);
  }
  public static void main(String[] args) {
    Main obj = new Main();
		obj.setLayout(new FlowLayout());
		obj.setVisible(true);
		obj.setSize(400,400);
  }
}
